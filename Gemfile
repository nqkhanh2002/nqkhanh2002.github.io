source 'https://rubygems.org'

gem "jekyll", "~> 4.3.3"
gem "jekyll-sass-converter", "~> 3.0"

group :jekyll_plugins do
  gem 'jekyll-feed'
  gem 'jekyll-sitemap'
  gem 'jemoji'
  gem 'jekyll-gist'
  gem 'jekyll-paginate'
end

gem 'webrick', '~> 1.8'
gem 'tzinfo-data'
gem 'tzinfo', '~> 1.2'

# Add wdm gem for Windows
gem 'wdm', '>= 0.1.0' if Gem.win_platform?